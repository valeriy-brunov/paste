{
    "name": "valeriy-brunov/paste",
    "description": "Вставляет внутрь себя при помощи AJAX-запроса HTML-код. Поддерживает разные режимы вставки и ожидания получения HTML-кода.",
    "type": "cakephp-plugin",
    "home": "https://github.com/valeriy-brunov/paste",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "webcomponent": ">=1.2.*",
        "cakephp/cakephp": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Paste\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paste\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
